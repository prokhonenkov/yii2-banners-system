{
    "name": "prokhonenkov/yii2-banners-system",
    "description": "The system of banners",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
      {
        "name": "Vitaliy Prokhonenkov",
        "email": "prokhonenkov@gmail.com"
      }
    ],
    "keywords": [
      "yii2",
      "extension",
      "banners"
    ],
    "minimum-stability": "stable",
    "require": {
      "php": ">=7.1",
      "yiisoft/yii2": "~2.0.0",
      "2amigos/yii2-ckeditor-widget" : "~2.1",
      "2amigos/yii2-grid-view-library": "~1.0",
      "prokhonenkov/yii2-repeater": "*"
    },
    "extra": {
      "bootstrap": "prokhonenkov\\bannerssystem\\Bootstrap"
    },
    "autoload": {
      "psr-4": {
        "prokhonenkov\\bannerssystem\\": "src"
      }
    },
    "require-dev": {
        "codeception/module-yii2": "^1.1",
        "codeception/module-asserts": "^1.1",
        "codeception/module-phpbrowser": "^1.0"
    }
}
